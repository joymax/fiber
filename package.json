{
  "name": "react-fiber",
  "version": "0.2.1",
  "description": "React components to use in chat, email applications.",
  "main": "lib/index.js",
  "scripts": {
    "start": "node devserver.js",
    "lint": "eslint --ext .js,.jsx .",
    "sass": "cpx './components/**/*.scss' ./lib",
    "babel": "babel components --out-dir lib",
    "build": "NODE_ENV=production npm run babel && npm run sass",
    "build:demo": "webpack --config webpack.config.js",
    "watch": "webpack --config webpack.config.js --watch",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reactbits/fiber.git"
  },
  "keywords": [
    "react",
    "chat",
    "email"
  ],
  "author": "Sergey Todyshev <stodyshev@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reactbits/fiber/issues"
  },
  "homepage": "https://github.com/reactbits/fiber#readme",
  "dependencies": {
    "halogen": "^0.1.10",
    "lodash": "^3.10.1",
    "md5": "^2.0.0",
    "moment": "^2.10.6",
    "query-string": "^3.0.0",
    "react": "^0.14.2",
    "react-imageloader": "^2.1.0",
    "react-markdown2": "^0.4.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.1.0",
