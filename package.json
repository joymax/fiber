    "eslint-plugin-react": "^3.7.1",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "got": "^5.1.0",
    "json-loader": "^0.5.3",
    "make-reducer": "^0.1.0",
    "node-sass": "^3.4.1",
